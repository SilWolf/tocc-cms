{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "name": "City",
    "displayName": "AI. 城市",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "code": {
      "type": "string",
      "private": false,
      "required": true,
      "unique": true
    },
    "shopName": {
      "type": "string",
      "required": true
    },
    "shopAddress": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "shopImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "level": {
      "type": "integer",
      "default": 1
    },
    "prosperity": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "prosperityMax": {
      "default": 0,
      "type": "integer",
      "required": true
    }
  }
}
